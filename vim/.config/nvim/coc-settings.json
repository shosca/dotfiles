{
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "coffee",
    "css",
    "graphql",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "python",
    "typescript",
    "typescriptreact"
  ],
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.snippetStatusText": "Ⓢ ",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "●",
  "diagnostic.infoSign": "•",
  "diagnostic.warningSign": "•",
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<cr>": [
      "expandable?",
      "expand",
      "open"
    ],
    "v": "open:vsplit"
  },
  "explorer.position": "floating",
  "explorer.previewAction.onHover": "labeling",
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.addedSign.text": "▎",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.changeRemovedSign.text": "▋",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.changedSign.text": "▎",
  "git.enableGutters": true,
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.removedSign.text": "▏",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.text": "▔",
  "git.virtualTextPrefix": " ❯❯❯ ",
  "highlight.disableLanguages": [
    "python"
  ],
  "html.autoClosingTags": true,
  "html.enable": true,
  "html.format.enable": true,
  "languageserver": {
    "bash": {
      "args": [
        "start"
      ],
      "command": "bash-language-server",
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    },
    "golang": {
      "command": "gopls",
      "filetypes": [
        "go"
      ],
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": [
        "terraform"
      ],
      "initializationOptions": {}
    }
  },
  "prettier.eslintIntegration": true,
  "prettier.statusItemText": "ⓟ ",
  "prettier.stylelintIntegration": true,
  "prettier.tslintIntegration": true,
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.maxNumberOfProblems": 999999,
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.sortImports.path": "zimports",
  "python.venvPath": "~/.local/share/virtualenvs",
  "suggest.completionItemKindLabels": {
    "class": "",
    "color": "",
    "constant": "",
    "default": "",
    "enum": "",
    "enumMember": "",
    "event": "鬒",
    "field": "",
    "file": "",
    "folder": "",
    "function": "⨕",
    "interface": "",
    "keyword": "",
    "method": "",
    "module": "",
    "operator": "Ψ",
    "property": "",
    "reference": "渚",
    "snippet": "",
    "struct": "פּ",
    "text": "♣",
    "typeParameter": "",
    "unit": "",
    "value": "",
    "variable": ""
  },
  "suggest.detailField": "abbr",
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.noselect": false,
  "suggest.timeout": 500,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.triggerCompletionWait": 100
}
